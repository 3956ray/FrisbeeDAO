{
  "name": "frisbeedao-api",
  "version": "1.0.0",
  "description": "FrisbeDAO Backend API Service",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "db:migrate": "node -r dotenv/config scripts/migrate.js",
    "db:seed": "node -r dotenv/config scripts/seed.js",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "generate:api-key": "node -r dotenv/config scripts/generate-api-key.js",
    "health-check": "curl http://localhost:3001/health"
  },
  "keywords": [
    "frisbeedao",
    "api",
    "blockchain",
    "social-tokens",
    "athletes"
  ],
  "author": "FrisbeDAO Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "viem": "^1.19.9",
    "axios": "^1.6.2",
    "mime-types": "^2.1.35",
    "uuid": "^9.0.1",
    "express-async-errors": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.10.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/mime-types": "^2.1.4",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-oauth2": "^1.4.15",
    "@types/express-session": "^1.17.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.2.2",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frisbeedao/backend-api.git"
  },
  "bugs": {
    "url": "https://github.com/frisbeedao/backend-api/issues"
  },
  "homepage": "https://github.com/frisbeedao/backend-api#readme"
}